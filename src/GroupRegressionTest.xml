<?xml version="1.0" encoding="UTF-8"?>

<suite name="mySuit" parallel="tests" thread-count="2">
    <test name="test1">

        <!-- Çalışıcak classların yazıldığı bölüm-->
        <classes>
            <parameter name="BrowserTipi" value="chrome"></parameter>
            <class name="US_204.TC_204"></class>
            <class name="US_205.TC_205"></class>
            <class name="US_206.TC_206"></class>
            <class name="US_208.TC_208"></class>
        </classes>

        <!-- Hangi grupların çalıştırılacağı bölüm-->
        <groups>
            <run>
                <include name="Regression"></include>
            </run>
        </groups>
    </test>
    <test name="test2">

        <!-- Çalışıcak classların yazıldığı bölüm-->
        <classes>
            <parameter name="BrowserTipi" value="edge"></parameter>
            <class name="US_204.TC_204"></class>
            <class name="US_205.TC_205"></class>
            <class name="US_206.TC_206"></class>
            <class name="US_208.TC_208"></class>
        </classes>

        <!-- Hangi grupların çalıştırılacağı bölüm-->
        <groups>
            <run>
                <include name="Regression"></include>
            </run>
        </groups>
    </test>
</suite>